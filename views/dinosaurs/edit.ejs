<form action="/dinosaurs/<%= dinoId %>/?_method=PUT" method="POST">
    <label for="name">Name</label>
    <input type="text" name="name" id="name" value="<%= dino.name %>">
    <label for="type">Type</label>
    <input type="text" name="type" id="type" value="<%= dino.type %>">
    <input type="submit" value="Submit">
</form>